{
  "cmake.configureArgs": [
    "-DCMAKE_EXPORT_COMPILE_COMMANDS=ON",
    "-DCMAKE_OSX_ARCHITECTURES=arm64",
    "-DCMAKE_SYSTEM_PROCESSOR=arm64",
    "-DDOBBY_BUILD_SILICON=ON",
    "-DCMAKE_VERBOSE_MAKEFILE=ON"
  ],
  "cmake.generator": "Ninja",
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "files.associations": {
    "*.d": "c",
    "filesystem": "cpp",
    "ios": "c",
    "__bit_reference": "cpp",
    "__node_handle": "cpp",
    "atomic": "cpp",
    "bitset": "cpp",
    "chrono": "cpp",
    "cstddef": "cpp",
    "deque": "cpp",
    "__memory": "cpp",
    "limits": "cpp",
    "locale": "cpp",
    "optional": "cpp",
    "ratio": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "vector": "cpp",
    "variant": "cpp",
    "iosfwd": "cpp",
    "istream": "cpp",
    "__locale": "cpp",
    "__hash_table": "cpp",
    "__split_buffer": "cpp",
    "__tree": "cpp",
    "array": "cpp",
    "initializer_list": "cpp",
    "map": "cpp",
    "set": "cpp",
    "stack": "cpp",
    "unordered_map": "cpp",
    "string": "cpp",
    "string_view": "cpp"
  },
  "cSpell.words": [
    "ringbuffer",
    "sysent"
  ],
  "lldb.verboseLogging": true,
  "lldb.showDisassembly": "auto",
  "lldb.dereferencePointers": true,
  "lldb.consoleMode": "commands",
  "debug.toolBarLocation": "docked"
}